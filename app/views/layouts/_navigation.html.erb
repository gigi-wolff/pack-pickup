<h2> Package Pick-up 
<%=# unless logged_in? 
   #   link_to 'Register', register_path, class: 'btn btn-primary btn-large'
   # end
%>
</h2>

<% if logged_in? %>
  <ul class="nav nav-tabs">
    <li class="active">
      <%= link_to('', class: 'btn btn-large dropdown-toggle', 'data-toggle' => 'dropdown') do %>
        <%= current_user.first_name %>
        <span class='caret'></span>
      <% end %>
      <ul class='dropdown-menu'>
        <li>
          <%= link_to edit_resident_path(current_user) do %>
            <i class='icon-pencil'></i>edit profile
          <% end %>
        </li>
        <li>
          <%= link_to resident_path(current_user) do %>
             <i class='icon-home'></i>
              show packages
          <% end %>
        </li>
      </ul>
    </li>
    <% if current_user.admin? %> 
      <li>
        <%= link_to 'All Residents', residents_path %>
      </li>
      <li>
        <%= link_to 'Add Resident', new_resident_path %>
      </li>
      <li>
        <%= link_to 'Apartments', apartments_path %>
      </li>    
    <% end %> 
    <p class='pull-right'> 
     <%= link_to 'Logout', logout_path, class: 'btn btn-danger btn-large'%> 
    </p>
  </ul> 
<% end %>

